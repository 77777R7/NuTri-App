{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-native",
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "*"
      ],
      "expo-router": [
        "node_modules/expo-router/build/index.d.ts"
      ],
      "expo-router/*": [
        "node_modules/expo-router/build/*"
      ],
      "expo-router/src/*": [
        "node_modules/expo-router/build/*"
      ]
    },
    "skipLibCheck": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "env.d.ts",
    "nativewind.d.ts",
    "nativewind-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "../legacy-web/**/*",
    "../archive/**/*",
    ".expo/types/**/*.ts"
  ]
}
